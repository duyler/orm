{
    "name": "duyler/orm",
    "type": "library",
    "description": "Duyler Cycle ORM integration library",
    "license": "MIT",
    "keywords": [
        "database",
        "db",
        "orm"
    ],
    "authors": [
        {
            "name": "Mikhail Ilinsky",
            "email": "mail@milinsky.ru"
        }
    ],
    "support": {
        "docs": "https://duyler.org/en/docs/orm/"
    },
    "require": {
        "php": "^8.3",
        "duyler/builder": "dev-main",
        "duyler/console": "dev-main",
        "cycle/orm": "^2.10",
        "cycle/entity-behavior-uuid": "^1.2",
        "ramsey/uuid": "^4.7",
        "cycle/migrations": "^4.2",
        "cycle/schema-migrations-generator": "^2.3",
        "nette/php-generator": "^4.1",
        "symfony/serializer": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "friendsofphp/php-cs-fixer": "^3.30",
        "vimeo/psalm": "^6.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Duyler\\ORM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Duyler\\ORM\\Test\\": ["tests/"]
        }
    }
}
